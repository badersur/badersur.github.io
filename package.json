{
    "name": "badersur.github.io",
    "private": true,
    "description": "A simple Progressive Web App using React and Gatsby",
    "version": "5.14.1",
    "author": "Bader Nasser <BaderSur@gmail.com>",
    "license": "MIT",
    "scripts": {
        "type-check": "tsc --noEmit",
        "clean": "gatsby clean",
        "lint:prettier": "prettier -c '**/*.{html,json,yml,yaml,css,md}'",
        "lint:eslint": "eslint . --ext .js,.jsx,.ts,.tsx",
        "lint": "run-s lint:*",
        "fmt": "run-s \"lint:prettier -- --write\" \"lint:eslint -- --fix\"",
        "prebuild": "run-s type-check lint clean",
        "build": "gatsby build",
        "postbuild": "echo \"\nSitemap: ${siteUrl}/sitemap.xml\" >> public/robots.txt",
        "predevelop": "run-s prebuild",
        "develop": "gatsby develop",
        "predevelop:docker": "docker-compose build",
        "develop:docker": "docker-compose up",
        "start": "gatsby develop",
        "preserve": "run-s build",
        "serve": "gatsby serve",
        "preserve:docker": "docker build -t bs-app .",
        "serve:docker": "docker run --rm --detach --publish 9000:9000 bs-app",
        "prebash:docker": "docker build -t bs-app .",
        "bash:docker": "docker-compose run --rm app bash",
        "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\""
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/badersur/badersur.github.io"
    },
    "bugs": {
        "url": "https://github.com/badersur/badersur.github.io/issues"
    },
    "dependencies": {
        "@fortawesome/fontawesome-svg-core": "^1.2.26",
        "@fortawesome/free-brands-svg-icons": "^5.12.0",
        "@fortawesome/free-solid-svg-icons": "^5.12.0",
        "@fortawesome/react-fontawesome": "^0.1.8",
        "commonmark": "^0.29.1",
        "gatsby": "^2.19.10",
        "gatsby-image": "^2.2.39",
        "gatsby-plugin-catch-links": "^2.1.24",
        "gatsby-plugin-google-analytics": "^2.1.34",
        "gatsby-plugin-manifest": "^2.2.40",
        "gatsby-plugin-offline": "^3.0.33",
        "gatsby-plugin-react-helmet": "^3.1.21",
        "gatsby-plugin-sharp": "^2.4.4",
        "gatsby-plugin-sitemap": "^2.2.26",
        "gatsby-plugin-typescript": "^2.1.26",
        "gatsby-source-filesystem": "^2.1.47",
        "gatsby-transformer-sharp": "^2.3.13",
        "gatsby-transformer-yaml": "^2.2.23",
        "react": "^16.12.0",
        "react-dom": "^16.12.0",
        "react-helmet": "^5.2.1"
    },
    "devDependencies": {
        "@babel/core": "^7.8.4",
        "@types/commonmark": "^0.27.4",
        "@types/node": "^13.5.3",
        "@types/react": "^16.9.19",
        "@types/react-dom": "^16.9.5",
        "@types/react-helmet": "^5.0.15",
        "@typescript-eslint/eslint-plugin": "^2.18.0",
        "@typescript-eslint/parser": "^2.18.0",
        "eslint": "^6.8.0",
        "eslint-config-airbnb": "^18.0.1",
        "eslint-config-prettier": "^6.10.0",
        "eslint-plugin-import": "^2.20.0",
        "eslint-plugin-jsx-a11y": "^6.2.3",
        "eslint-plugin-prettier": "^3.1.2",
        "eslint-plugin-react": "^7.18.0",
        "eslint-plugin-react-hooks": "^1.7.0",
        "graphql": "^14.6.0",
        "npm-run-all": "^4.1.5",
        "prettier": "^1.19.1",
        "typescript": "^3.7.5"
    }
}
