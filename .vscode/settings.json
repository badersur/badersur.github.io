{
	"eslint.enable": false,
	"editor.formatOnSave": true,
	"editor.codeActionsOnSave": {
		"source.fixAll": true
	},
	"files.exclude": {
		"**/.git": true,
		"**/.svn": true,
		"**/.hg": true,
		"**/CVS": true,
		"**/.DS_Store": true,
		".cache": true,
		"node_modules": true,
		"public": true,
		"package-lock.json": true,
		"client-secret.json.enc": true,
		"yarn.lock": true
	},
	"xo.enable": true,
	"xo.format.enable": true
}
